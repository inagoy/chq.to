<%=  render 'links/link', link: link %>
<div class="row justify-content-start px-4">
    <span class="badge p-3 rounded-pill text-bg-warning col-1"> Ephemeral </span>
</div>
<% if !link.accessible? %>
<div class="w-50 mx-auto">
    <hr>
    <div class="text-danger">This link expired</div>
    <hr>
</div>
<% end %>